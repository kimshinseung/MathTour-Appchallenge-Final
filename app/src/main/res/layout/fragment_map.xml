<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MapFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="93dp"
            android:background="#CFE9E4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginStart="15dp"
            android:layout_marginTop="4dp"
            android:fontFamily="@font/font1"
            android:text="체험하고 싶은 장소를 선택하세요!"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/textView" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginStart="15dp"
            android:layout_marginTop="19dp"
            android:fontFamily="@font/font1"
            android:text="전체 코스"
            android:textColor="@color/black"
            android:textSize="25sp"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/imageView3"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="90sp"
            android:layout_height="90sp"
            android:layout_marginStart="16dp"
            android:src="@mipmap/choice3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="410dp"
            android:layout_height="650sp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="1dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="369dp"
                android:layout_height="500dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="102dp"
                android:background="@drawable/allmap4"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/susan"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="4dp"
                    android:layout_marginTop="128dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toEndOf="@+id/taean"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/sejong"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="2dp"
                    android:layout_marginTop="40dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toEndOf="@+id/susan"
                    app:layout_constraintTop_toBottomOf="@+id/suwon" />

                <ImageView
                    android:id="@+id/daegu"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="96dp"
                    android:layout_marginTop="27dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toEndOf="@+id/junju"
                    app:layout_constraintTop_toBottomOf="@+id/youngju" />

                <ImageView
                    android:id="@+id/junju"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="88dp"
                    android:layout_marginTop="36dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/susan" />

                <ImageView
                    android:id="@+id/gawngju"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="72dp"
                    android:layout_marginTop="24dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/junju" />

                <ImageView
                    android:id="@+id/dasan"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="112dp"
                    android:layout_marginBottom="100dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintBottom_toTopOf="@+id/jeju"
                    app:layout_constraintStart_toStartOf="parent" />

                <ImageView
                    android:id="@+id/jeju"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="16dp"
                    android:layout_marginBottom="16dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <ImageView
                    android:id="@+id/tongyuong"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="92dp"
                    android:layout_marginTop="60dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toEndOf="@+id/gawngju"
                    app:layout_constraintTop_toBottomOf="@+id/daegu" />

                <ImageView
                    android:id="@+id/busan"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="36dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toEndOf="@+id/tongyuong"
                    app:layout_constraintTop_toBottomOf="@+id/ulsan" />

                <ImageView
                    android:id="@+id/ulsan"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="148dp"
                    android:layout_marginTop="164dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toEndOf="@+id/junju"
                    app:layout_constraintTop_toBottomOf="@+id/gyungpodae" />

                <ImageView
                    android:id="@+id/taean"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="27dp"
                    android:layout_marginTop="130dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/gyungbuk"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="88dp"
                    android:layout_marginTop="38dp"
                    android:src="@drawable/pin4"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/chungbuk"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="68dp"
                    android:layout_marginTop="115dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toEndOf="@+id/susan"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/suwon"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="128dp"
                    android:layout_marginTop="53dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/youngju"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="56dp"
                    android:layout_marginTop="68dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toEndOf="@+id/sejong"
                    app:layout_constraintTop_toBottomOf="@+id/gyungpodae" />

                <ImageView
                    android:id="@+id/gyungpodae"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="37dp"
                    android:layout_marginTop="28dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toEndOf="@+id/chunchun"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/chunchun"
                    android:layout_width="40sp"
                    android:layout_height="40sp"
                    android:layout_marginStart="47dp"
                    android:layout_marginTop="24dp"
                    android:src="@drawable/pin3"
                    app:layout_constraintStart_toEndOf="@+id/gyungbuk"
                    app:layout_constraintTop_toTopOf="parent" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>